@{
    ViewBag.App = "Cadê Meu Médico ?";
}
<!DOCTYPE html>
<html lang="pt-br">
<head>
    <meta name="viewport" content="width=device-width,initial-scale=1.0, maximum-scale=1.0, user-scalable=no">
    <title>@ViewBag.App - @ViewBag.Title</title>
    <link href="@Url.Content("~/Content/bootstrap.min.css")" rel="stylesheet" />
    <style>
        body {
            padding-top: 75px;
        }

        .top10 {
            padding-top: 10px;
        }
    </style>
</head>
<body>
    <nav class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="/">@ViewBag.App</a>
            </div>
            <div id="navbar" class="collapse navbar-collapse">
                <ul class="nav navbar-nav">
                    <li>@Html.ActionLink("Área Mobile [Teste]", "Index", "Painel", new { Area = "Mobile" }, new { })</li>
                    <li>@Html.ActionLink("Cidades", "Index", "Cidade")</li>
                    <li>@Html.ActionLink("Especialidades", "Index", "Especialidade")</li>
                    <li>@Html.ActionLink("Médicos", "Index", "Medicos")</li>
                    <li>@Html.ActionLink("Usuários", "Index", "Usuario")</li>
                </ul>
            </div><!--/.nav-collapse -->
        </div>
    </nav>
    <div class="container">
        @RenderBody()
    </div>
    <script src="@Url.Content("~/Scripts/jquery-3.1.0.min.js")">

    </script>
    <script src="@Url.Content("~/Scripts/bootstrap.min.js")">

    </script>
    <script src="@Url.Content("~/Scripts/jquery.validate.min.js")">

    </script>
    @RenderSection("script", required: false)
</body>
</html>